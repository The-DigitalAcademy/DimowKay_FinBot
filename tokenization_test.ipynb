    "def get_embedding(text):\n",
    "    qinputs = tokenizer(text, return_tensors=\"pt\", padding=True, truncation=True)\n",
    "    with torch.no_grad():\n",
    "         aoutputs = model(**qinputs)\n",
    "    return aoutputs.last_hidden_state.mean(dim=1).squeeze().numpy()\n",
    "\n",
